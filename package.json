{
  "name": "hit-u-bad",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -p next:dev postcss:watch",
    "build": "npm-run-all postcss:build next:build",
    "next:dev": "next dev",
    "next:build": "next build && next export && node scripts/generate_sitemap_xml.js",
    "start": "next start",
    "postcss:build": "postcss src/styles/index.css -o src/styles/bundle.css",
    "postcss:watch": "postcss src/styles/index.css -o src/styles/bundle.css -w"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@tailwindcss/custom-forms": "^0.2.1",
    "firebase": "^7.22.1",
    "gray-matter": "^4.0.2",
    "marked": "^1.1.1",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "tailwindcss": "^1.6.2",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/marked": "^1.1.0",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.0.0",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "sass": "^1.26.10",
    "typescript": "^3.9.7"
  }
}
